    border-left: none; /* Removes the vertical line before the arrow */
}

/* 4. The Arrow Icon (Blue chevron) */
QComboBox::down-arrow {
    image: url(&quot;program/assets/global/down_blue_arrow.svg&quot;);
    width: 14px;
padding-right: 15px;
    height: 14px;
}

/* 5. Styling the Popup List (The part that drops down) */
QAbstractItemView {
    outline: 0;
    border-radius: 0px;
