            <property name="styleSheet">
             <string notr="true">/* 1. The Main Table Container */
QTableWidget {
    background-color: white;
    border: 1px solid #d1d9e6;
    border-radius: 12px; /* Keeps the outer box rounded */
    gridline-color: transparent;
    outline: 0;
	padding:0;
}

/* 2. The Header Container - Key fix here */
QHeaderView {
    background-color: #e6f0f9; 
    border: none;
    border-bottom-left-radius: 0px; /* Matches the outer table curve */
    border-bottom-right-radius: 0px;
}

/* 3. Individual Header Sections - Removing the internal radius */
QHeaderView::section {
    background-color: #e6f0f9;
    color: #334155;
    font-weight: bold;
    font-size: 13px;
    padding: 8px;
	padding-left: 0px;
	padding-right: 0px;
    
    /* Force square corners for the internal blue blocks */
    border-radius: 0px; 
    
    /* Clean separator lines */
    border: none;
    border-right: 1px solid #cbd5e1; 
}

/* Remove the line from the very last header section */
QHeaderView::section:last {
    border-right: none;
}

/* 4. Table Items (Cells) */
QTableWidget::item {
    color: #1e293b;
	padding-left: 25px;
    border-bottom: 1px solid #f1f5f9; 
}

/* 5. Selection style */
QTableWidget::item:selected {
    background-color: #dbeafe;
    color: #1e40af;
}

/* 6. Fix for the empty top-left/top-right corners */
QTableWidget QTableCornerButton::section {
    background-color: #e6f0f9;
    border: none;
    border-top-left-radius: 10px;
}</string>
